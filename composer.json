{
    "name": "tlegenbayangali/progress-bar",
    "description": "This is my package progress-bar",
    "keywords": [
        "tlegenbayangali",
        "laravel",
        "progress-bar"
    ],
    "homepage": "https://github.com/tlegenbayangali/progress-bar",
    "support": {
        "issues": "https://github.com/tlegenbayangali/progress-bar/issues",
        "source": "https://github.com/tlegenbayangali/progress-bar"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Tlegen Bayangali",
            "email": "tlegenbayangali@yandex.kz",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.1",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "TlegenBayangali\\ProgressBar\\": "src/",
            "TlegenBayangali\\ProgressBar\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TlegenBayangali\\ProgressBar\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TlegenBayangali\\ProgressBar\\ProgressBarServiceProvider"
            ],
            "aliases": {
                "ProgressBar": "TlegenBayangali\\ProgressBar\\Facades\\ProgressBar"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}